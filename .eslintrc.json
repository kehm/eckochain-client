{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "import/extensions": "off",
        "new-cap": "warn",
        "no-param-reassign": "warn",
        "no-shadow": "warn",
        "no-unused-vars": "warn"
    }
}
